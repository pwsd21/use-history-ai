{
  "name": "use-history-ai",
  "version": "1.0.2",
  "description": "AI-powered clipboard history manager for React with Google Gemini integration",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external react",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch --external react",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "react-hooks",
    "hooks",
    "clipboard",
    "clipboard-manager",
    "history",
    "ai",
    "gemini",
    "google-gemini",
    "llm",
    "productivity"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pwsd21/use-history-ai.git"
  },
  "bugs": {
    "url": "https://github.com/pwsd21/use-history-ai/issues"
  },
  "homepage": "https://github.com/pwsd21/use-history-ai#readme",
  "author": {
    "name": "Pawan Sachdeva",
    "email": "pawansachdeva1998@gmail.com",
    "url": "https://github.com/pwsd21"
  },
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^0.3.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
